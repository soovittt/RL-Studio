{
  "name": "rl-studio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "pre-pr": "bash scripts/pre-pr-check.sh",
    "pre-pr:frontend": "npm ci && npm run lint && npm run format -- --check && npx tsc --noEmit && npm test -- --run && npm run build",
    "pre-pr:backend": "cd backend && python3 -m pip install -q flake8 black isort mypy pytest pytest-cov && flake8 rl_studio --count --exit-zero --max-complexity=10 --max-line-length=127 --statistics && black --check rl_studio && isort --check-only rl_studio && mypy rl_studio --ignore-missing-imports --no-strict-optional || true && pytest --cov=rl_studio --cov-report=term",
    "dev": "vite",
    "build": "vite build",
    "start": "vite preview",
    "test": "vitest",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "dependencies": {
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@react-three/postprocessing": "^2.19.1",
    "@sentry/react": "^7.99.0",
    "@sentry/vite-plugin": "^2.15.0",
    "@tanstack/react-query": "^5.56.0",
    "@tanstack/react-router": "^1.45.0",
    "@tanstack/router-devtools": "^1.45.0",
    "@tanstack/router-vite-plugin": "^1.45.0",
    "@tanstack/start": "^1.0.0",
    "convex": "^1.15.0",
    "graphql": "^16.12.0",
    "graphql-request": "^7.3.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.12.0",
    "three": "^0.181.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.0",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "vinxi": "^0.4.0",
    "vite": "^5.3.4",
    "vitest": "^1.6.0"
  }
}
